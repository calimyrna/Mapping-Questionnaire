<h1>Listing usermaps</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Address</th>
  </tr>

<% @usermaps.each do |usermap| %>
  <tr>
    <td><%=h usermap.email %></td>
    <td><%=h usermap.address %></td>
    <td><%= link_to 'Show', usermap %></td>
    <td><%= link_to 'Edit', edit_usermap_path(usermap) %></td>
    <td><%= link_to 'Destroy', usermap, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New usermap', new_usermap_path %>